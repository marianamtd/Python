valores = []
num = 0
for c in range(0, 6):
    num = int(input('Digite um valor: '))
    if num % 2 == 0:
        valores[0].append(num)
    else:
        valores[1].append(num)
valores[1].sort()
valores[0].sort()
print(f'Os valores pares digitados foram: {valores[0]}'
      f'\nOs valores Ã­mpares digitados foram: {valores[1]}')