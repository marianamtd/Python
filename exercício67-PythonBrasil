print('\033[1;30m<_>'*20)
while True:
    tab = int(input('Quer ver a tabuada de qual valor: '))
     if tab < 0:
        break
    for c in range(1,11,1):
     print(f'{tab} X {c} = {tab*c}')
print('acabou')
