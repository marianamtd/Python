fut = ('São Paulo', 'Atlético', 'Flamengo', 'Inter', 'Grêmio', 'Palmeiras',
       'Fluminense', 'Santos', 'Corinthias', 'Ceará')
print(f'Lista de times do Brasileirão: {fut}')
print('-='*15)
print(f'Os 5 primeiros são {fut[0:5]}')
print('-='*15)
print(f'Os ultimos 4 são {fut[-4:]}')
print('-='*15)
print(f'Em ordem alfabética: {sorted(fut)}')
print('-=' *15)
print(f'O Fluminense está na {fut.index("Fluminense") + 1}ª posição')